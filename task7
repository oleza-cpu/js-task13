    <script type="text/javascript">

function deleteEvenNumbers(array) {
    return array.filter(function(num) {
        return num % 2 !== 0;
    });
}

var arr = [1, 2, 3, 4, 5];
arr = deleteEvenNumbers(arr);
console.log(arr); // [1, 3, 5]


        </script>
